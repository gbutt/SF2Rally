<apex:page standardStylesheets="false" sidebar="false" showHeader="false" applyBodyTag="false" applyHtmlTag="false" docType="html-5.0" standardController="Case">
<html lang="en">
<head>  
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1"/>
	<title>Case to Rally</title>
	<link rel="stylesheet" href="{!URLFOR($Resource.Angular, 'bootstrap/dist/css/bootstrap.min.css')}" />

